<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                   https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="Contacts\Domain\Organization\Organization" repository-class="Contacts\Infrastructure\ORM\Repository\ORMOrganizationRepository">
        <id name="id" type="organization_id">
            <generator strategy="NONE"/>
        </id>
        <field name="name" type="string"/>
        <field name="email" type="email"/>
        <field name="phoneNumber" type="phone_number"/>
        <embedded name="address" class="Contacts\Domain\Value\Address"/>
    </entity>

</doctrine-mapping>
